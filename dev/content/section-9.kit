<section>
    <div>
        <img src="img/Section-9/Employee-1.png" alt="Employee-1">
        <img src="img/Section-9/employee-2.png" alt="Employee-2">
        <h1>
            Seeking a Job?
        </h1>
        <form>
            <h2>Apply Here!</h2>
            <label>First and Last Name</label>
            <input type="text" placeholder="First and last name ">
            <label>Email</label>
            <input type="text" placeholder="Name@anyemail.com">
            <label type="number">Phone Number</label>
            <input type="text" placeholder="(xxx) xxx-xxx">
            <label>Resume</label>
            <textarea>Add your resume here</textarea>
            <button>Upload</button>
            <label>Select position</label>
            <select>
              <option>Cashier</option>
              <option>Manager</option>
            </select>
    </div>
</section>